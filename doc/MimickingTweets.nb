Cell["\<\
Given a string result so far and a list of binary digits, return a 2-tuple of \
the result with appended encoded characters and remaining inbits.\
\>", "Text",
 CellChangeTimes->{{3.590241948143951*^9, 3.590242000005934*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"HuffmanEncodeIter", "[", 
   RowBox[{"result_", ",", "inbits_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"With", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"nls", "=", 
      RowBox[{"NextLetters", "[", "result", "]"}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"Switch", "[", 
     RowBox[{"nls", ",", "\[IndentingNewLine]", 
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{"HuffmanEncodeIter", "[", 
       RowBox[{
        RowBox[{"StringJoin", "[", 
         RowBox[{"result", ",", 
          RowBox[{"Seed", "[", "]"}]}], "]"}], ",", "inbits"}], "]"}], ",", 
      "\[IndentingNewLine]", "_", ",", "\[IndentingNewLine]", 
      RowBox[{"With", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"q", "=", 
          RowBox[{"HuffmanEncode", "[", 
           RowBox[{
            RowBox[{"Huffman", "[", "nls", "]"}], ",", "inbits"}], "]"}]}], 
         "}"}], ",", "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"StringJoin", "[", 
           RowBox[{"result", ",", 
            RowBox[{"q", "[", 
             RowBox[{"[", "1", "]"}], "]"}]}], "]"}], ",", 
          RowBox[{"q", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], "}"}]}], "]"}]}], 
     "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", "]"}]}]], "Input",\

 CellChangeTimes->{{3.5902419082913227`*^9, 3.5902419413454113`*^9}, {
  3.590242016581006*^9, 3.5902420652763157`*^9}, {3.5902422549210863`*^9, 
