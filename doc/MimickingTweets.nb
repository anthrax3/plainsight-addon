Cell["Weight List", "Section",
 CellChangeTimes->{{3.589890433204339*^9, 3.58989043505145*^9}}],

Cell[TextData[{
 "When we choose an entry by random, we need to make the choice with respect \
to the existing probabilities. In ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 ", we use the function RandomChoice which expects input in the form ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     SubscriptBox[
      StyleBox["w", "TI"], 
      StyleBox["1", "TR"]], ",", 
     SubscriptBox[
      StyleBox["w", "TI"], 
      StyleBox["2", "TR"]], ",", 
     StyleBox["\[Ellipsis]", "TR"]}], "}"}], "->", 
   RowBox[{"{", 
    RowBox[{
     SubscriptBox[
      StyleBox["e", "TI"], 
      StyleBox["1", "TR"]], ",", 
     SubscriptBox[
      StyleBox["e", "TI"], 
      StyleBox["2", "TR"]], ",", 
     StyleBox["\[Ellipsis]", "TR"]}], "}"}]}]], "InlineFormula"],
 "."
}], "Text",
 CellChangeTimes->{{3.589890445872364*^9, 3.5898905237964287`*^9}, {
  3.58989055753382*^9, 3.589890574941654*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"ToWeightList", "[", "list_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"Last", "/@", "list"}], ")"}], "\[Rule]", 
   RowBox[{"(", 
    RowBox[{"First", "/@", "list"}], ")"}]}]}]], "Input",
 CellChangeTimes->{{3.5898905836982*^9, 3.589890679620206*^9}, {
  3.589890892997472*^9, 3.5898909121957808`*^9}, {3.58989114151982*^9, 
  3.589891176353902*^9}}]
}, Closed]],

