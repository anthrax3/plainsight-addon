Cell["\<\
Takes a string and returns a list of binary digits (integer zero and one) \
based on the 16-bit (UTF-16) decoding of each character. JavaScript\
\[CloseCurlyQuote]s charCodeAt is 16-bit, so the two versions should be \
compatible.\
\>", "Text",
 CellChangeTimes->{{3.5902409198145227`*^9, 3.590240969896751*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"ToBinaryString", "[", "plaintext_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Flatten", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"IntegerDigits", "[", 
      RowBox[{
       RowBox[{"ToCharacterCode", "[", "#", "]"}], ",", "2", ",", "16"}], 
      "]"}], "&"}], "/@", 
    RowBox[{"StringSplit", "[", 
     RowBox[{"plaintext", ",", "\"\<\>\""}], "]"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.590239911826462*^9, 3.590239918551066*^9}, {
  3.5902403996160316`*^9, 3.590240486398889*^9}}],

Cell["\<\
Note: compressing the binary string represenation of plaintext would be a \
good idea; particularly as there are long sections of padded zeros for most \
English text.\
\>", "Text",
 CellChangeTimes->{{3.5902410104854393`*^9, 3.5902410455563993`*^9}}],

Cell["\<\
Encode takes plaintext, a global link to a ntable, and then: transforms \
plaintext to a binary string, seeds the output text, iteratively generates a \
frequency table for the next character choices given the output text until \
all the binary plain text is consumed.\
\>", "Text",
 CellChangeTimes->{{3.590241439586858*^9, 3.590241517136489*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"StegEncrypt", "[", "plaintext_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"First", "[", "\[IndentingNewLine]", 
   RowBox[{"NestWhile", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"HuffmanEncodeIter", "[", 
       RowBox[{
        RowBox[{"#", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", 
        RowBox[{"#", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], "]"}], "&"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Seed", "[", "]"}], ",", 
       RowBox[{"ToBinaryString", "[", "plaintext", "]"}]}], "}"}], " ", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{"add", " ", "to", " ", "out"}], ",", " ", 
       RowBox[{"consume", " ", "in"}]}], " ", "*)"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"Length", "[", 
        RowBox[{"#", "[", 
         RowBox[{"[", "2", "]"}], "]"}], "]"}], ">", "0"}], "&"}]}], 
    "\[IndentingNewLine]", "]"}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.590241263432991*^9, 3.590241309323868*^9}, {
  3.590241523523489*^9, 3.5902416853898277`*^9}, {3.5902417896254807`*^9, 
  3.590241812811816*^9}, {3.590241842829358*^9, 3.590241856822887*^9}, {
