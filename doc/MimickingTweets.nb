Cell["Condensing Tables", "Section",
 CellChangeTimes->{{3.589889852082498*^9, 3.589889855890079*^9}}],

Cell["\<\
Treating each character as a separate item in the list is wasteful. Let\
\[CloseCurlyQuote]s convert them to 2-tuples, with the first entry be a \
string with n characters.\
\>", "Text",
 CellChangeTimes->{{3.5898898605326643`*^9, 3.589889917498962*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Collapse", "[", "table_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Table", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"StringJoin", "[", 
       RowBox[{
        RowBox[{"table", "[", 
         RowBox[{"[", "record", "]"}], "]"}], "[", 
        RowBox[{"[", 
         RowBox[{"1", ";;", 
          RowBox[{
           RowBox[{"Length", "[", 
            RowBox[{"table", "[", 
             RowBox[{"[", "record", "]"}], "]"}], "]"}], "-", "1"}]}], "]"}], 
        "]"}], "]"}], ",", 
      RowBox[{
       RowBox[{"table", "[", 
        RowBox[{"[", "record", "]"}], "]"}], "[", 
       RowBox[{"[", 
        RowBox[{"-", "1"}], "]"}], "]"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{"record", ",", 
      RowBox[{"Length", "[", "table", "]"}]}], "}"}]}], "\[IndentingNewLine]",
    "]"}]}]], "Input",
 CellChangeTimes->{{3.589889960974855*^9, 3.589889988715287*^9}, {
  3.589890023558723*^9, 3.589890079109847*^9}, {3.5898901264821777`*^9, 
  3.58989014085028*^9}, {3.589890232413336*^9, 3.5898902340483313`*^9}}],

