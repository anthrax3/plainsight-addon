Cell["Generating Text", "Section",
 CellChangeTimes->{{3.5898897671087914`*^9, 3.5898897689790983`*^9}}],

Cell["\<\
To simplify the API, two global variables:\
\>", "Text",
 CellChangeTimes->{{3.589891965255769*^9, 3.589891971755199*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"\[GothicCapitalT]", "=", "n3table"}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"table", " ", "reference"}], " ", "*)"}]}], "\[IndentingNewLine]", 

 RowBox[{
  RowBox[{
   RowBox[{"\[GothicCapitalN]", "=", 
    RowBox[{"StringLength", "[", 
     RowBox[{
      RowBox[{"\[GothicCapitalT]", "[", 
       RowBox[{"[", "1", "]"}], "]"}], "[", 
      RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"number", " ", "of", " ", "characters", " ", "in", " ", "string"}],
    " ", "*)"}]}]}], "Input",
 CellChangeTimes->{{3.58989197509445*^9, 3.58989202534121*^9}, {
  3.58989207350207*^9, 3.589892101010398*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Seed", "[", "]"}], ":=", 
  RowBox[{"RandomChoice", "[", 
   RowBox[{"ToWeightList", "[", "\[GothicCapitalT]", "]"}], "]"}]}]], "Input",\

 CellChangeTimes->{{3.589899729126211*^9, 3.58989975040432*^9}}],

Cell["\<\
To initiate a text, we first choose an entry at random. This is the seed.\
\>", "Text",
 CellChangeTimes->{{3.589889777932209*^9, 3.589889788366445*^9}, {
  3.589891225352955*^9, 3.5898912346737957`*^9}}],

Cell["\<\
Once we have a seed (or text that has built upon the seed), we can add a \
letter to the text by looking a the n-1 characters from our table and \
choosing the next probabilistically.\
\>", "Text",
 CellChangeTimes->{{3.5898915106865273`*^9, 3.589891516688569*^9}, {
  3.5898915688543158`*^9, 3.589891640017591*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"NextLetters", "[", "text_String", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Select", "[", 
    RowBox[{"\[GothicCapitalT]", ",", 
     RowBox[{
      RowBox[{"StringMatchQ", "[", 
       RowBox[{
        RowBox[{"#", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", 
        RowBox[{
         RowBox[{"StringTake", "[", 
          RowBox[{"text", ",", 
           RowBox[{
            RowBox[{"-", "\[GothicCapitalN]"}], "+", "1"}]}], "]"}], "~~", 
         "_"}]}], "]"}], "&"}]}], "]"}], "/.", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"s_String", ",", "w_Integer"}], "}"}], ":>", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"StringTake", "[", 
       RowBox[{"s", ",", 
        RowBox[{"-", "1"}]}], "]"}], ",", "w"}], "}"}]}]}]}]], "Input",
 CellChangeTimes->{{3.589891657129046*^9, 3.5898916606667643`*^9}, {
   3.589891695850525*^9, 3.589891698949524*^9}, {3.589891841642104*^9, 
   3.589891850775077*^9}, {3.5898918934662123`*^9, 3.5898919449560633`*^9}, 
   3.5898921074185038`*^9, {3.5898922260721817`*^9, 3.5898922409840193`*^9}, {
   3.589892301460462*^9, 3.589892322309211*^9}, {3.5898923586479273`*^9, 
   3.589892399638214*^9}, {3.5898951110004272`*^9, 3.589895196472206*^9}, {
   3.58989524122508*^9, 3.589895245834878*^9}, {3.589895290051251*^9, 
   3.589895315618051*^9}, {3.589912516806294*^9, 3.589912517721663*^9}, {
   3.589914452670718*^9, 3.5899144530288057`*^9}, {3.589914587388625*^9, 
   3.589914638188713*^9}, {3.589914844700776*^9, 3.589914848263733*^9}, {
