Cell[CellGroupData[{

Cell["Steganographic Encryption", "Section",
 CellChangeTimes->{{3.59017079229543*^9, 3.590170795172125*^9}, {
  3.590262563234688*^9, 3.590262569230393*^9}}],

Cell["\<\
Let a transversal to the left represent 0 and a transversal to the right \
represent 1. Then, given a Huffman tree and a binary string, return a token \
from the tree and the unused binary characters.\
\>", "Text",
 CellChangeTimes->{{3.590170822118648*^9, 3.590170885830336*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"HuffmanEncode", "[", 
   RowBox[{"htree_", ",", 
    RowBox[{"{", "}"}]}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Switch", "[", 
   RowBox[{
    RowBox[{"htree", "[", 
     RowBox[{"[", "2", "]"}], "]"}], ",", "\[IndentingNewLine]", "\"\<\>\"", 
    ",", 
    RowBox[{"(*", " ", 
     RowBox[{"non", "-", 
      RowBox[{"terminal", " ", "node", " ", "in", " ", "tree"}]}], " ", 
     "*)"}], "\[IndentingNewLine]", 
    RowBox[{"HuffmanEncode", "[", 
     RowBox[{
      RowBox[{"htree", "[", 
       RowBox[{"[", "1", "]"}], "]"}], ",", 
      RowBox[{"{", "}"}]}], "]"}], ",", " ", 
    RowBox[{"(*", " ", 
     RowBox[{
     "effectively", " ", "pad", " ", "zeros", " ", "if", " ", "no", " ", 
      "further", " ", "digits", " ", "to", " ", "encode"}], " ", "*)"}], 
    "\[IndentingNewLine]", "_String", ",", " ", 
    RowBox[{"(*", " ", 
     RowBox[{"reached", " ", "terminal", " ", "character"}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"htree", "[", 
       RowBox[{"[", "2", "]"}], "]"}], ",", 
      RowBox[{"{", "}"}]}], "}"}]}], "\[IndentingNewLine]", "]"}]}]], "Input",\

 CellChangeTimes->{{3.5901729388857594`*^9, 3.590173060458275*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"HuffmanEncode", "[", 
   RowBox[{"htree_", ",", "binarylist_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Switch", "[", 
   RowBox[{
    RowBox[{"htree", "[", 
     RowBox[{"[", "2", "]"}], "]"}], ",", "\[IndentingNewLine]", "\"\<\>\"", 
    ",", 
    RowBox[{"(*", " ", 
     RowBox[{"non", "-", 
      RowBox[{"terminal", " ", "node", " ", "in", " ", "tree"}]}], " ", 
     "*)"}], "\[IndentingNewLine]", 
    RowBox[{"Switch", "[", 
     RowBox[{
      RowBox[{"First", "[", "binarylist", "]"}], ",", "\[IndentingNewLine]", 
      "0", ",", "\[IndentingNewLine]", 
      RowBox[{"HuffmanEncode", "[", 
       RowBox[{
        RowBox[{"htree", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", 
        RowBox[{"Rest", "[", "binarylist", "]"}]}], "]"}], ",", 
      "\[IndentingNewLine]", "1", ",", "\[IndentingNewLine]", 
      RowBox[{"HuffmanEncode", "[", 
       RowBox[{
        RowBox[{"htree", "[", 
         RowBox[{"[", "4", "]"}], "]"}], ",", 
        RowBox[{"Rest", "[", "binarylist", "]"}]}], "]"}], ",", 
      "\[IndentingNewLine]", "_", ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Print", "[", 
        RowBox[{"\"\<Found non-binary digit \>\"", ",", 
         RowBox[{"Head", "[", "binarylist", "]"}], ",", 
         "\"\< in binarylist\>\""}], "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"Abort", "[", "]"}]}]}], "\[IndentingNewLine]", "]"}], ",", 
    "\[IndentingNewLine]", "_String", ",", 
    RowBox[{"(*", " ", 
     RowBox[{"reached", " ", "terminal", " ", "character"}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"htree", "[", 
       RowBox[{"[", "2", "]"}], "]"}], ",", "binarylist"}], "}"}], " ", ",", 
    "\[IndentingNewLine]", "_", ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
     "Print", "[", "\"\<Found non-string character/empty string in htree\>\"",
       "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Abort", "[", "]"}]}]}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{"should", " ", "not", " ", "reach"}], " ", "*)"}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.590170898900312*^9, 3.590170933185993*^9}, {
  3.590170993376412*^9, 3.590171095945447*^9}, {3.590171189474745*^9, 
